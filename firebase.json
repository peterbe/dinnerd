{
  "database": {
    "rules": "database-rules.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "./build",
    "ignore": [
      "firebase.json",
      "database-rules.json",
      "storage.rules"
    ]
  },
  "headers": [
    {
      "source" : "static/*.@(jpg|jpeg|gif|png|svg)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "max-age=7200"
        }
      ]
    }
  ]
}
